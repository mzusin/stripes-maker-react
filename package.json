{
  "name": "stripes-maker-react",
  "version": "1.0.0",
  "description": "A free online SVG stripe pattern generator and maker. Create multi-stripe animated patterns, copy CSS or SVG code that is ready for you web projects, or just download the SVG file for free!",
  "type": "module",
  "scripts": {
    "build:website": "node run/website/index.js",
    "watch:website": "node run/website/index.js -- watch",
    "eslint": "eslint ./src/**",
    "typescript-declarations": "tsc --emitDeclarationOnly"
  },
  "keywords": [
    "stripes",
    "crochet",
    "knitting",
    "svg",
    "animated svg"
  ],
  "author": "Miriam Zusin <miriam.zusin@gmail.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mzusin/stripes-maker-react"
  },
  "dependencies": {
    "darkly": "^1.0.19",
    "lodash-es": "^4.17.21",
    "mz-math": "^3.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@types/lodash-es": "^4.17.7",
    "@types/react-dom": "^18.2.1",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "autoprefixer": "^10.4.14",
    "esbuild": "^0.17.18",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "fs-extra": "^11.1.1",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "uglifycss": "^0.0.29"
  }
}
